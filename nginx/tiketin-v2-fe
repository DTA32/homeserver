server {
        
	root /var/www/tiketin-v2/frontend/dist;
        index index.html index.htm index.nginx-debian.html;

        server_name tiketin.dta32.my.id;

        location / {
                try_files $uri $uri/ /index.html;
        }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/tiketin.dta32.my.id/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/tiketin.dta32.my.id/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = tiketin.dta32.my.id) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


        listen 80;

        server_name tiketin.dta32.my.id;
    return 404; # managed by Certbot


}