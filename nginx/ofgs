server {
	server_name dfgs.dta32.my.id;

	location /_next/static {
		add_header Cache-Control "public, max-age=3600, immutable";
		alias /var/www/ofgs/.next/static;
	}

	location / {
		proxy_pass	http://127.0.0.1:9998;
		proxy_read_timeout	60;
		proxy_connect_timeout	60;
		proxy_redirect		off;
		proxy_http_version 1.1;
		proxy_set_header Upgrade $http_upgrade;
		proxy_set_header Connection 'upgrade';
		proxy_set_header Host $host;
		proxy_cache_bypass $http_upgrade;
	}

	

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/dfgs.dta32.my.id/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/dfgs.dta32.my.id/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = dfgs.dta32.my.id) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


	listen 80;
	server_name dfgs.dta32.my.id;
    return 404; # managed by Certbot


}
